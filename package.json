{
  "name": "zenflow-lint-js",
  "version": "2.0.0",
  "description": "A zero-config opinionated and awesome js linter",
  "engines": {
    "node": ">=6"
  },
  "bin": "bin/cmd.js",
  "files": [
    "bin",
    "scripts",
    "util",
    ".eslintrc.json",
    "CHANGELOG.md"
  ],
  "scripts": {
    "fix": "node bin/cmd.js --fix",
    "lint": "node bin/cmd.js",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenflow/zenflow-lint-js.git"
  },
  "keywords": [
    "lint",
    "linter",
    "check",
    "checker",
    "code",
    "style",
    "eslint",
    "standard"
  ],
  "author": {
    "name": "Matthew Francis Brunetti",
    "email": "zenflow87@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zenflow/zenflow-lint-js/issues"
  },
  "homepage": "https://github.com/zenflow/zenflow-lint-js#readme",
  "dependencies": {
    "deep-equal": "^1.0.1",
    "eslint": "^4.18.2",
    "eslint-config-zenflow": "^2.1.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^2.0.3",
    "standard-engine": "^8.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.1",
    "@semantic-release/git": "^4.0.1",
    "semantic-release": "^15.0.3"
  }
}
